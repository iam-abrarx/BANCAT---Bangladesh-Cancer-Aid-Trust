{
    "name": "bancat-frontend",
    "homepage": "https://rootmacts.github.io/BANCAT---Bangladesh-Cancer-Aid-Trust/",
    "version": "1.0.0",
    "description": "BANcat Frontend - React SPA for cancer awareness and patient support platform",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "predeploy": "npm run build && cd ../admin && npm install && npm run build",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@hookform/resolvers": "^3.3.2",
        "@mui/icons-material": "^5.14.19",
        "@mui/material": "^5.14.20",
        "@tanstack/react-query": "^5.14.2",
        "@tanstack/react-query-devtools": "^5.14.2",
        "@types/canvas-confetti": "^1.9.0",
        "axios": "^1.6.2",
        "canvas-confetti": "^1.9.4",
        "clsx": "^2.0.0",
        "date-fns": "^3.0.0",
        "framer-motion": "^10.16.16",
        "i18next": "^23.7.8",
        "i18next-browser-languagedetector": "^7.2.0",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-h5-audio-player": "^3.9.1",
        "react-helmet-async": "^2.0.4",
        "react-hook-form": "^7.49.2",
        "react-i18next": "^13.5.0",
        "react-router-dom": "^6.20.0",
        "swiper": "^11.0.5",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/lodash": "^4.14.202",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.0.4",
        "@vitest/ui": "^1.0.4",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.3.0",
        "jsdom": "^23.0.1",
        "prettier": "^3.1.1",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vitest": "^1.0.4"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "author": "BANcat Team",
    "license": "MIT"
}